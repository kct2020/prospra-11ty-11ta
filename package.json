{
  "name": "prospra-11ty-11ta",
  "version": "1.0.0",
  "description": "Source for PROSPRA.com",
  "author": "Keith Taylor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kct2020/prospra-11ty-11ta/issues"
  },
  "homepage": "https://github.com/kct2020/prospra-11ty-11ta#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kct2020/prospra-11ty-11ta.git"
  },
  "keywords": [
    "11ty",
    "TailwindCSS",
    "TailwindUI",
    "Alpine.js",
    "webpack",
    "PostCSS",
    "CSSnano"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev:webpack": "webpack  --mode development",
    "dev:eleventy": "eleventy --serve",
    "dev:postcss": "postcss src/assets/css/tailwind.css -o dist/assets/tailwind.css --watch",
    "dev": "npm-run-all clean --parallel dev:* --print-label",
    "build:webpack": "webpack  --mode production",
    "build:eleventy": "ELEVENTY_ENV=production eleventy",
    "build:postcss": "NODE_ENV=production postcss src/assets/css/tailwind.css -o dist/assets/tailwind.css",
    "build": "run-s clean build:* --print-label"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "0.11.1",
    "@11ty/eleventy-navigation": "0.3.2",
    "@11ty/eleventy-plugin-rss": "1.1.0",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.0.6",
    "@babel/core": "7.12.13",
    "@babel/preset-env": "7.12.13",
    "@tailwindcss/custom-forms": "0.2.1",
    "alpinejs": "2.8.0",
    "autoprefixer": "9.8.8",
    "axios": "0.21.1",
    "babel-loader": "8.2.2",
    "clean-css": "5.2.1",
    "cssnano": "4.1.10",
    "dotenv": "8.2.0",
    "html-minifier": "4.0.0",
    "luxon": "1.25.0",
    "markdown-it": "12.2.0",
    "markdown-it-emoji": "1.4.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "7.1.2",
    "postcss-import": "12.0.1",
    "rimraf": "3.0.2",
    "slugify": "1.4.6",
    "svg-sprite": "1.5.0",
    "tailwindcss": "1.9.6",
    "terser-webpack-plugin": "4.2.3",
    "webpack": "4.46.0",
    "webpack-cli": "3.3.12"
  }
}
